{
  "name": "sveltekit-ecommerce",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "preview": "vite preview",
    "build": "nx exec -- vite build",
    "prepare": "nx exec --  svelte-kit sync",
    "typecheck": "nx exec -- svelte-check --tsconfig ./tsconfig.json",
    "lint": "nx exec -- eslint .",
    "fmt": "nx exec -- prettier --write ."
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@sveltejs/adapter-auto": "^2.1.0",
    "@sveltejs/kit": "^1.22.3",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.32.4",
    "nx": "16.5.5",
    "nx-cloud": "latest",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.1",
    "svelte": "^4.1.1",
    "svelte-check": "^3.4.6",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.7"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{projectRoot}/.svelte-kit"
        ]
      },
      "check": {
        "outputs": [
          "{projectRoot}/.svelte-kit"
        ]
      }
    }
  }
}
